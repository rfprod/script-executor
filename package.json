{
  "name": "script-executor",
  "version": "1.0.0",
  "description": "Continuous JS-TS-script executor. An interviewing tool alternative to a whiteboard.",
  "repository": "https://github.com/rfprod/script-executor.git",
  "license": "MIT",
  "author": "rfprod",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx sort-package-json",
    "lint": "npm run lint:js; npm run lint:ts",
    "lint:js": "eslint --ext .js ./",
    "lint:ts": "eslint --ext .ts ./",
    "prestart": "npm run clear-ts-build",
    "start": "gulp",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@types/node": "14.14.16",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "cheerio": "1.0.0-rc.5",
    "colors": "1.4.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-watch": "7.0.0",
    "gulp": "4.0.2",
    "prettier": "2.2.1",
    "request": "2.88.0",
    "sort-package-json": "1.48.0",
    "typescript": "4.1.3"
  }
}
